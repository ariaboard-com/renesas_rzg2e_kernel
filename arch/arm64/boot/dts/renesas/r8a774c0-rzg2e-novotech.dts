/dts-v1/;
#include "r8a774c0.dtsi"
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "Silicon Linux RZ/G2E Customized board based on EK874";
	compatible = "si-linux,cat875", "si-linux,cat874", "renesas,r8a774c0";

	aliases {
		serial0 = &scif2;
		serial1 = &hscif2;
		serial2 = &hscif3;
		serial3 = &hscif4;
		spi0 = &msiof0;
		spi1 = &msiof1;
		spi2 = &msiof2;
		spi3 = &msiof3;
		ethernet0 = &avb;
	};

	chosen {
		bootargs = "rootfstype=ext4 ro rootwait";
		stdout-path = "serial0:115200n8";
	};

	memory@48000000 {
		device_type = "memory";
		/* first 128MB is reserved for secure area. */
		reg = <0x0 0x48000000 0x0 0x78000000>;
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		/* global autoconfigured region for contiguous allocations */
		linux,cma@58000000 {
			compatible = "shared-dma-pool";
			reusable;
			reg = <0x00000000 0x58000000 0x0 0x10000000>;
			linux,cma-default;
		};

		/* device specific region for contiguous allocations */
		mmp_reserved: linux,multimedia {
			compatible = "shared-dma-pool";
			reusable;
			reg = <0x00000000 0x68000000 0x0 0x08000000>;
		};
	};


	mmngr {
		compatible = "renesas,mmngr";
		memory-region = <&mmp_reserved>;
	};

	mmngrbuf {
		compatible = "renesas,mmngrbuf";
	};

	vspm_if {
		compatible = "renesas,vspm_if";
	};

	vcc_sdhi0: regulator-vcc-sdhi0 {
		compatible = "regulator-fixed";

		regulator-name = "SDHI0 Vcc";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
		regulator-boot-on;
	};

	vccq_sdhi0: regulator-vccq-sdhi0 {
		compatible = "regulator-fixed";

		regulator-name = "SDHI0 VccQ";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
		regulator-boot-on;
	};

	vcc_emmc: regulator-vcc-emmc {
		compatible = "regulator-fixed";

		regulator-name = "eMMC Vcc";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
		regulator-boot-on;
	};

	vccq_emmc: regulator-vccq-emmc {
		compatible = "regulator-fixed";

		regulator-name = "eMMC VccQ";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
		regulator-boot-on;
	};

	wlan_en_reg: fixedregulator {
		compatible = "regulator-fixed";
		regulator-name = "wlan-en-regulator";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
		regulator-boot-on;
	};

	ov5640_vdddo_1v8: 1p8v {
		compatible = "regulator-fixed";
		regulator-name = "camera_vdddo";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	ov5640_vdda_2v8: 2p8v {
		compatible = "regulator-fixed";
		regulator-name = "camera_vdda";
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		regulator-always-on;
	};

	ov5640_vddd_1v5: 1p5v {
		compatible = "regulator-fixed";
		regulator-name = "camera_vddd";
		regulator-min-microvolt = <1500000>;
		regulator-max-microvolt = <1500000>;
		regulator-always-on;
	};

	osc25250_clk: osc25250_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <24000000>;
	};

	sound {
		compatible = "simple-audio-card";
		simple-audio-card,format = "i2s";
		simple-audio-card,mclk-fs = <256>;

		#address-cells = <1>;
		#size-cells = <0>;

		simple-audio-card,dai-link@1 {
			reg = <1>;
			format = "i2s";
			cpu {
				sound-dai = <&rcar_sound 1>;
				bitclock-master;
				frame-master;
			};

			codec {
				sound-dai = <&rt5616>;
			};
		};

	};

	/* External DU dot clocks */
	x13_clk: x13 {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <74250000>;
	};

	hdmi-out {
		compatible = "hdmi-connector";
		type = "a";

		port {
			hdmi_con_out: endpoint {
				remote-endpoint = <&ch7033_out>;
			};
		};
	};

	vdd_bl_reg: bl-regulator {
		compatible = "regulator-fixed";
		regulator-name = "fixed-12V";
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
		regulator-boot-on;
		regulator-always-on;
	};

	backlight {
		compatible = "pwm-backlight";
		pwms = <&pwm5 0 5000000>;

		brightness-levels = <0 4 8 16 32 64 128 255>;
		default-brightness-level = <6>;

		power-supply = <&vdd_bl_reg>;
		enable-gpios = <&gpio1 7 GPIO_ACTIVE_HIGH>;
	};

        clocks {
		rt5616_mclk: rt5616_mclk {
			compatible = "fixed-clock";
			#clock-cells = <0>;
			clock-frequency = <11289600>;
			clock-output-names = "rt5616-mclk";
			status = "okay";
                };
        };

	osc25250_clk: osc25250_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <24000000>;
	};

	sc16is7xx_clk: sc16is7xx_clk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <1843200>;
	};

	panel-lvds {
		compatible = "advantech,idk-2121wr", "panel-lvds";

		width-mm = <476>;
		height-mm = <268>;

		data-mapping = "vesa-24";

		panel-timing {
			clock-frequency = <65000000>;
			hactive = <1024>;
			vactive = <768>;
			hsync-len = <136>;
			hfront-porch = <24>;
			hback-porch = <160>;
			vfront-porch = <3>;
			vback-porch = <29>;
			vsync-len = <6>;
		};

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				dual-lvds-odd-pixels;
				panel_in0: endpoint {
					remote-endpoint = <&lvds0_out>;
				};
			};
		};
	};
};

&audio_clk_a {
	clock-frequency = <22579200>;
};

&extal_clk {
	clock-frequency = <48000000>;
};

&pcie_bus_clk {
	clock-frequency = <100000000>;
};

&pciec0 {
	/* Map all possible DDR as inbound ranges */
	dma-ranges = <0x42000000 0 0x40000000 0 0x40000000 0 0x80000000>;
};

&pfc {
	scif2_pins: scif2 {
		groups = "scif2_data_a";
		function = "scif2";
	};

	hscif2_pins: hscif2 {
		groups = "hscif2_data_a", "hscif2_ctrl_a";
		function = "hscif2";
	};

	hscif3_pins: hscif3 {
		groups = "hscif3_data_c", "hscif3_ctrl_c";
		function = "hscif3";
	};

	hscif4_pins: hscif4 {
		groups = "hscif4_data_b";
		function = "hscif4";
	};

	sdhi0_pins: sd0 {
		groups = "sdhi0_data4", "sdhi0_ctrl";
		function = "sdhi0";
		power-source = <3300>;
	};
	pwm5_pins: pwm5 {
		groups = "pwm5_a";
		function = "pwm5";
	};

	pwm6_pins: pwm6 {
		groups = "pwm6_a";
		function = "pwm6";
	};

	sound_clk_pins: sound_clk {
		groups = "audio_clkout_a", "audio_clkout1_a",
			 "audio_clk_a", "audio_clk_b_a", "audio_clk_c_a";
		function = "audio_clk";
	};

	sound_pins: sound {
		groups = "ssi01239_ctrl", "ssi0_data",
			 "ssi349_ctrl", "ssi3_data", "ssi4_data";
		function = "ssi";
	};

	tpu0_pins: tpu {
		groups = "tpu0_to0", "tpu0_to1";
		function = "tpu0";
	};

	usb0_pins: usb {
		groups = "usb0_b";
		function = "usb0";
	};

	usb30_pins: usb30 {
		groups = "usb30", "usb30_id";
		function = "usb30";
	};

	sdhi3_pins: sd3 {
		groups = "sdhi3_data8", "sdhi3_ctrl";
		function = "sdhi3";
		power-source = <3300>;
	};

	sdhi3_pins_uhs: sd3_uhs {
		groups = "sdhi3_data8", "sdhi3_ctrl";
		function = "sdhi3";
		power-source = <1800>;
	};

	du_pins: du {
		groups = "du_rgb888", "du_sync", "du_clk_out_0",
			"du_disp";
		function = "du";
	};

	msiof0_pins: spi0 {
		groups = "msiof0_clk", "msiof0_sync",
			"msiof0_rxd", "msiof0_txd";
		function = "msiof0";
	};

	avb_pins: avb {
		mux {
			groups = "avb_mii";
			function = "avb";
		};
	};

	can0_pins: can0 {
		groups = "can0_data";
		function = "can0";
	};

	can1_pins: can1 {
		groups = "can1_data";
		function = "can1";
	};


	sdhi1_pins: sd1 {
		groups = "sdhi1_data4", "sdhi1_ctrl";
		function = "sdhi1";
		power-source = <3300>;
	};

	camera_pins: camera {
		
	};
};

&scif2 {
	pinctrl-0 = <&scif2_pins>;
	pinctrl-names = "default";

	status = "okay";
};

&hscif2 {
	pinctrl-0 = <&hscif2_pins>;
	pinctrl-names = "default";

	uart-has-rtscts;
	status = "okay";
};

&hscif3 {
	pinctrl-0 = <&hscif3_pins>;
	pinctrl-names = "default";

	uart-has-rtscts;
	status = "okay";
};

&sdhi3 {
	/* used for on-board eMMC */
	status = "okay";
	pinctrl-0 = <&sdhi3_pins>;
	pinctrl-1 = <&sdhi3_pins_uhs>;
	pinctrl-names = "default", "state_uhs";

	vmmc-supply = <&vcc_emmc>;
	vqmmc-supply = <&vccq_emmc>;
	bus-width = <8>;

	non-removable;
	fixed-emmc-driver-type = <1>;
	max-frequency = <200000000>;
	mmc-hs200-1_8v;
};

&sdhi0 {
	pinctrl-0 = <&sdhi0_pins>;
	pinctrl-names = "default";

	/* No 1.8V supply avaiable for SD card on this board. */
	vmmc-supply = <&vcc_sdhi0>;
	vqmmc-supply = <&vccq_sdhi0>;
	cd-gpios = <&gpio3 12 GPIO_ACTIVE_LOW>;
	bus-width = <4>;
	max-frequency = <52000000>;
	cap-sd-highspeed;
	no-1-8-v;
	status = "okay";
};

&sdhi1 {
	status = "okay";
	pinctrl-0 = <&sdhi1_pins>;
	pinctrl-names = "default";

	vmmc-supply = <&wlan_en_reg>;
	bus-width = <4>;
	max-frequency = <52000000>;
	non-removable;
	cap-power-off-card;
	keep-power-in-suspend;	
	no-1-8-v;

        bcmdhd: bcmdhd@0 {
                compatible = "android,bcmdhd_wlan";
                status = "okay";
        };

/*
	brcmf: wifi@0 {
		reg = <0>;
		compatible = "brcm,bcm4329-fmac";
                status = "okay";
	};
*/
};

&pwm5 {
	pinctrl-0 = <&pwm5_pins>;
	pinctrl-names = "default";

	status = "okay";
};

&pwm6 {
	pinctrl-0 = <&pwm6_pins>;
	pinctrl-names = "default";

	status = "okay";
};

&cmt0 {
	status = "okay";
};

&cmt1 {
	status = "okay";
};

&tmu0 {
	status = "okay";
};

&tmu1 {
	status = "okay";
};

&tmu2 {
	status = "okay";
};

&tmu3 {
	status = "okay";
};

&tmu4 {
	status = "okay";
};

&rcar_sound {
	pinctrl-0 = <&sound_pins &sound_clk_pins>;
	pinctrl-names = "default";

	/* Single DAI */
	#sound-dai-cells = <1>;

	/* audio_clkout0/1/2/3 */
	#clock-cells = <1>;
	clock-frequency = <11289600>;

	status = "okay";

	rcar_sound,dai {
		dai0 {
			playback = <&ssi0 &src0 &dvc0>;
		};

		dai1 {
			playback = <&ssi4 &src4 &dvc1>;
			capture  = <&ssi3 &src3 &dvc1>;
		};
	};
};

&ssi4 {
	shared-pin;
};

&ehci0 {
	dr_mode = "host";
	status = "okay";
};

&ohci0 {
	dr_mode = "host";
	status = "okay";
};

&usb2_phy0 {
	pinctrl-0 = <&usb0_pins>;
	pinctrl-names = "default";

	status = "okay";
};

&xhci0 {
	pinctrl-0 = <&usb30_pins>;
	pinctrl-names = "default";
	dr_mode = "host";

	status = "okay";
};

&usb3_peri0 {
	companion = <&xhci0>;

	usb-role-switch;
	role-switch-default-mode = "host";

	status = "okay";
};

&vspb {
	status = "okay";
};

&vspi0 {
	status = "okay";
};

&du {
	pinctrl-0 = <&du_pins>;
	pinctrl-names = "default";
	status = "okay";

	clocks = <&cpg CPG_MOD 724>,
		 <&cpg CPG_MOD 723>,
		 <&x13_clk>;
	clock-names = "du.0", "du.1", "dclkin.0";

	ports {
		port@0 {
			du_out_rgb: endpoint {
				remote-endpoint = <&ch7033_in>;
			};
		};
	};
};

&vin4 {
	status = "okay";
};

&vin5 {
	status = "okay";
};

&csi40 {
	status = "okay";
};

&i2c3 {
	status = "okay";
};

&tpu{
	pinctrl-0 = <&tpu0_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&hscif4 {
	pinctrl-0 = <&hscif4_pins>;
	pinctrl-names = "default";

	uart-has-rtscts;
	status = "okay";
};

&rwdt {
	status = "okay";
	timeout-sec = <60>;
};

&i2c0 {
	status = "okay";
	clock-frequency = <100000>;

	rt5616: rt5616@1b {
		compatible = "realtek,rt5616";
		reg = <0x1b>;
		clocks = <&rt5616_mclk>;
		#sound-dai-cells = <0>;
	};

	rx8025: rtc@32 {
		compatible = "dallas,rx8025";
		reg = <0x32>;
		trickle-resistor-ohms = <250>;
	};

	ch7033: ch7033@76 {
		compatible = "chrontel,ch7033";
		reg = <0x76>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				ch7033_in: endpoint {
					remote-endpoint = <&du_out_rgb>;
				};
			};

			port@1 {
				reg = <1>;
				ch7033_out: endpoint {
					remote-endpoint = <&hdmi_con_out>;
				};
			};
		};
	};

	ov5640_csi40: camera@3c {
		compatible = "ovti,ov5640";
		reg = <0x3c>;
		pinctrl-0 = <&camera_pins>;
		pinctrl-names = "default";
		clock-names = "xclk";
		clocks = <&osc25250_clk>;
		clock-frequency = <24000000>;
		DOVDD-supply = <&ov5640_vdddo_1v8>;
		AVDD-supply = <&ov5640_vdda_2v8>;
		DVDD-supply = <&ov5640_vddd_1v5>;
		reset-gpios = <&gpio5 16 GPIO_ACTIVE_LOW>;
		virtual-channel = /bits/ 8 <0>;

		port {
			ov5640_csi40_ep: endpoint {
				clock-lanes = <0>;
				data-lanes = <1 2>;
				remote-endpoint = <&csi40_in>;
			};
		};
	};

};

&msiof0 {
	pinctrl-0 = <&msiof0_pins>;
	pinctrl-names = "default";
	status = "okay";

	sc16is762: sc16is762@0 {
		compatible = "nxp,sc16is762";
		reg = <0>;
		spi-max-frequency = <15000000>;
		clocks = <&sc16is7xx_clk>;
		interrupt-parent = <&gpio1>;
		interrupts = <5 IRQ_TYPE_EDGE_FALLING>;
		gpio-controller;
		#gpio-cells = <2>;
	};
};

&avb {
	pinctrl-0 = <&avb_pins>;
	pinctrl-names = "default";
	renesas,no-ether-link;
	phy-handle = <&phy0>;
	phy-mode = "rgmii";
	status = "okay";

	phy0: ethernet-phy@0 {
		rxc-skew-ps = <1500>;
		reg = <0>;
		interrupt-parent = <&gpio2>;
		interrupts = <21 IRQ_TYPE_LEVEL_LOW>;
		reset-gpios = <&gpio1 20 GPIO_ACTIVE_LOW>;
	};
};

&can0 {
	pinctrl-0 = <&can0_pins>;
	pinctrl-names = "default";
	renesas,can-clock-select = <0x0>;
	status = "okay";
};

&can1 {
	pinctrl-0 = <&can1_pins>;
	pinctrl-names = "default";
	renesas,can-clock-select = <0x0>;
	status = "okay";
};

&pciec0 {
	status = "okay";
};

&lvds0 {
	status = "okay";

	clocks = <&cpg CPG_MOD 727>,
		 <&x13_clk>;
	clock-names = "fck", "dclkin.0";

	ports {
		port@1 {
			lvds0_out: endpoint {
				remote-endpoint = <&panel_in0>;
			};
		};
	};
};

&lvds1 {
	status = "okay";

	clocks = <&cpg CPG_MOD 727>,
		 <&x13_clk>;
	clock-names = "fck", "dclkin.0";
};

&gpio3 {
        wl-reg-on-gpio{
                gpio-hog;
                gpios = <14 GPIO_ACTIVE_HIGH>;
                output-high;
                line-name = "wl-reg-on-gpio";
        };
};

&csi40 {
	status = "okay";

	ports {
		port {
			csi40_in: endpoint {
				clock-lanes = <0>;
				data-lanes = <1 2>;
				remote-endpoint = <&ov5640_csi40_ep>;
			};
		};
	};
};

